{
  "LoggingSettings": {
    "Loggers": [
      {
        "LoggerName": "ConsoleLogger",
        "LogOutputLevel": "Info"
      },
      {
        "LoggerName": "FileLogger",
        "FileLogPath": "Data/Output/errors.txt",
        "LogOutputLevel": "Error"
      }
    ]
  },
  "ProblemGeneratorInputProviderSettings": {
    "InputFolderPath": "Data/Input/TestInput",
    "InputFilePrefix": "input",
    "FileExtension": "txt"
  },
  "InferenceRuleProviderSettings": {
    "RuleFolderPath": "Data/Rules",
    "FileExtension": "txt"
  },
  "SimplificationRuleProviderSettings": {
    "FilePath": "Data/simplifications.txt"
  },
  "ObjectIntroductionRuleProviderSettings": {
    "FilePath": "Data/object_introductions.txt"
  },
  "ProblemGenerationRunnerSettings": {
    "WriteJsonBestTheoremFile": true,
	"JsonBestTheoremFilePath": "Data/Output/best_theorems.json",
    "TakeAtMostOneInterestingTheoremPerConfiguration": true
  },
  "ProblemGeneratorSettings": {
    "NumberOfPictures": 3,
    "ExcludeAsymmetricConfigurations": true
  },
  "TheoremSorterSettings": {
    "NumberOfTheorems": 500
  },
  "TheoremFindingSettings": {
    "TangentCirclesTheoremFinderSettings": {
      "ExcludeTangencyInsidePicture": true
    },
    "LineTangentToCircleTheoremFinderSettings": {
      "ExcludeTangencyInsidePicture": true
    },
    "SoughtTheoremTypes": [
      "CollinearPoints",
      "ConcurrentLines",
      "ConcyclicPoints",
      "EqualLineSegments",
      "LineTangentToCircle",
      "ParallelLines",
      "PerpendicularLines",
      "TangentCircles",
      "Incidence"
    ]
  },
  "TheoremRankingSettings": {
    "TheoremRankerSettings": {
      "RankingCoefficients": {
        "Symmetry": 1000,
        "Simplification": 150,
        "Circles": -5,
        "Repetition": -1,
        "Theorems": -1,
        "TheoremType": 1,
        "Constructions": 1
      }
    },
    "TheoremTypeRankerSettings": {
      "TypeRankings": {
        "TangentCircles": 100,
        "LineTangentToCircle": 100,
        "ConcurrentLines": 20,
        "Incidence": -5
      }
    },
    "ConstructionsRankerSettings": {
      "ConstructionRankings": {
        "IntersectionOfLineAndLineFromPoints": -20,
        "IntersectionOfLines": -20,
        "IntersectionOfLinesFromPoints": -20,
        "TangentLine": 5
      }
    }
  },
  "GenerationSettings": {
    "ConfigurationFilterType": "MemoryEfficient"
  },
  "TraceSortingGeometryFailures": true,
  "SortingGeometryFailureTracerSettings": {
    "FailureFilePath": "Data/Output/sorting_geometry_failures.txt"
  }
}